<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.complitex.flexbuh.common.service.UserSessionBean">
    <select id="selectSelectedPersonProfileName" parameterType="long" resultType="string">
        select `profile_name` from `person_profile` where `selected` = true and `session_id` = #{sessionId}
    </select>
</mapper>